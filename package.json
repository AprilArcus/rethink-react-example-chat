  "devDependencies": {
    "babel": "~5.8.23",
    "babel-core": "~5.8.25",
    "babel-eslint": "~4.1.3",
    "babel-loader": "~5.3.2",
    "babel-plugin-react-transform": "~1.1.1",
    "css-loader": "~0.19.0",
    "eslint": "~1.6.0",
    "eslint-loader": "~1.1.0",
    "eslint-plugin-react": "~3.5.1",
    "mocha": "~2.3.3",
